{
  "version": 1,
  "projects": {
    "countdowner": {
      "root": "apps/countdowner",
      "sourceRoot": "apps/countdowner/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "start": {
          "builder": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "run-ios": {
          "builder": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "builder": "@nrwl/react-native:bundle",
          "outputs": ["apps/countdowner/build"],
          "options": {
            "entryFile": "apps/countdowner/index.js",
            "platform": "ios",
            "bundleOutput": "dist/apps/countdowner/ios/index.bundle"
          }
        },
        "run-android": {
          "builder": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "builder": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/countdowner/android/app/build/outputs/bundle",
            "apps/countdowner/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "builder": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/countdowner/index.js",
            "platform": "android",
            "bundleOutput": "dist/apps/countdowner/android/index.bundle"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/countdowner/**/*.{js,ts,tsx}"]
          }
        },
        "sync-deps": {
          "builder": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/countdowner/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "store": {
      "root": "libs/store",
      "sourceRoot": "libs/store/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/store/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/store/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "defaultProject": "countdowner"
}
